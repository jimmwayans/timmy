nmap -sTVC 10.11.1.5 -Pn > nmap.txt
nmap 10.11.1.5 -p 445 --script=smtp-*
nc -nvC 10.11.1.5 445

nmap -v -p 139, 445 --script=smb-os-discovery 10.11.1.5 > smb.txt
