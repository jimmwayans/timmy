nmap -sV -sC -Pn 10.11.1.136 > nmap.txt
nmap --script vuln -sV -p 22,113,139,445 10.11.1.136 > nmapvuln.txt
nmap --script smb* -sV -p 139,445 10.11.1.136 > nmapsmb.txt
nmap -p- 10.11.1.136 > nmapall.txt


